{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/CardGames.Core/**.csproj",
            "src/CardGames.War/**.csproj"
          ]
        }
      ],
      "dest": "libraries",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "libraries/**.yml",
          "libraries/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "README.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "*.png"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/material"
    ],
    "globalMetadata": {
      "newFileRepository": {
        "branch": "main"
      },
      "_appFooter": "Copyright (c) - Arsslen Idadi 2020. All rights reserved.",
      "_appTitle": "War Card Games Documentation",
      "_appLogoPath": "logo.png"
    },
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}